<template>
<askcontainer>1</askcontainer>
<description>New Samba3 Machine</description>
<icon>server.png</icon>
<invalid>0</invalid>
<rdn>cn</rdn>
<!--<regexp>^ou=People,o=.*,</regexp>-->
<title>Samba: Machine</title>
<visible>1</visible>

<objectClasses>
<!-- <objectClass id="account"></objectClass> -->
<objectClass id="device"></objectClass>
</objectClasses>

<attributes>
<attribute id="cn">
	<display>Machine Name</display>
	<hint>The name of the machine</hint>
	<icon>host.png</icon>
	<onchange>=autoFill(cn;%uid%)</onchange>
	<order>1</order>
	<page>1</page>
</attribute>
<attribute id="description">
        <display>Description</display>
        <hint>The machine name should end with a $</hint>
        <icon>terminal.png</icon>
        <onchange>=autoFill(cn;%uid%)</onchange>
        <order>2</order>
        <page>1</page>
</attribute>
<attribute id="serialnumber">
        <display>Serial Number</display>
        <hint>The machine name should end with a $</hint>
        <icon>terminal.png</icon>
        <onchange>=autoFill(cn;%uid%)</onchange>
        <order>3</order>
        <page>1</page>
</attribute>
<attribute id="l">
        <display>Location</display>
        <hint>The machine name should end with a $</hint>
        <icon>terminal.png</icon>
        <onchange>=autoFill(cn;%uid%)</onchange>
        <order>4</order>
        <page>1</page>
</attribute>
<attribute id="owner">
        <display>Owner</display>
        <hint>The machine name should end with a $</hint>
        <icon>ldap-user.png</icon>
        <onchange>=autoFill(cn;%uid%)</onchange>
        <order>5</order>
        <page>1</page>
</attribute>
<!--<attribute id="cn">
	<hidden>1</hidden>
	<value></value>
</attribute> -->
</attributes>

</template>
